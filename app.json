{
  "expo": {
    "name": "strike-pay",
    "slug": "strike-pay",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "assets": ".assets",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#5A8DFE"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#5A8DFE"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow Strike wallet to use Face ID.",
          "NSFaceIDUsageDescription":"Protect your funds with Face ID, ensuring only you can access your crypto wallet."
        }
      ]
    ]
  }
}
